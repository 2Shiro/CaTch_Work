<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.catwork.mapper.PersonMapper">  

<select id="getPersonInfo">
	SELECT 
		PER_IDX,
		USER_IDX,
		NAME,
		PHONE,
		ZIP_CODE,
		ADDRESS,
		SOCIAL_NUM,
		CREATED
	FROM PERSON_TB
		
		

</select>



</mapper>